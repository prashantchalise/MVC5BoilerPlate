@{
	ViewData["Title"] = "Welcome to Rolpo-Tech Boilerplate:: A product of Rolpo";
	ViewBag.Current = "Index";
}


 <div class="content-wrapper" data-ng-controller='dashboardController'>
	<!-- Content Header (Page header) -->
	<section class="content-header">
		<h1>
			Dashboard
			<small>1.0</small>
		</h1>
		<ol class="breadcrumb">
			<li><a href="@Url.Action("Dashboard", "Home")"><i class="fa fa-dashboard"></i> Home</a></li>
			<li class="active">Dashboard</li>
		</ol>



	</section>
	<!-- Main content -->
	<section class="content">
		<!-- Info boxes -->
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12">
			</div><!-- /.col -->

		</div><!-- /.row -->
		<!-- Main row -->

		<div class="row">
			<div class="col-md-12">
				<div class="box box-danger">
					<div class="box-header">
						<h3 class="box-title">My Tasks</h3>
  					</div><!-- /.box-header -->
					<div class="box-body no-padding">
						 
						<table class="table table-bordered table-hover  table-condensed table-font-siz">
							<thead>
								<tr>
									<th>QT#</th>
									<th>Task</th>
									<th>Service</th>
								</tr>
							</thead>

							<tbody>
								<tr>
									<td colspan="3" class="text-center">
										<p><b>On  the scale of North-korea to America, How free are you? </b><b><i>Canada!! :P</i></b></p>
									</td>
								</tr>
							</tbody>
						</table>
					</div><!-- /.box-body -->
					 
				</div>
			</div><!-- /.col -->
			 
		</div><!-- /.row --> 

	</section><!-- /.content -->
</div><!-- /.content-wrapper -->
